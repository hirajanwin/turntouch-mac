  # Uncomment the next line if you're using Swift or would like to use dynamic frameworks
  use_frameworks!
  pod 'AFNetworking', '~> 3.0'
  pod 'Sparkle'
  
  # Pods for Turn Touch Mac

  target 'Turn Touch Mac Tests' do
    inherit! :search_paths
    # Pods for testing
  end

end
